package Creational.FactoryMethod.Restaurant;

public class BeefBurger implements Burger {
    private String name;
    private int price;
    private String code;

    public BeefBurger() {
        this.name = "Beef Burger";
        this.price = 10;
        this.code = "BB_8001"

    }

    @Override
    public void prepare() {
        System.out.println("Preparing " + name + " with price " + price + " and code " + code);
    }
}
